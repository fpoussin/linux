CONFIG_FW_LOADER_USER_HELPER_FALLBACK=y

#
# Default contiguous memory area size:
#
CONFIG_CMA_SIZE_MBYTES=128

#
# Disk-On-Chip Device Drivers
#

#
# LPDDR & LPDDR2 PCM memory drivers
#

#
# Misc devices
#
CONFIG_SRAM=y

#
# SCSI support type (disk, tape, CD-ROM)
